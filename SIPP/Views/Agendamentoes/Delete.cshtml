@model SIPP.Models.Agendamento

@{
    ViewData["Title"] = "Delete";
}


<div class="container mt-4">

<h3 class="mt-4">Tem certeza que deseja excluir o agendamento?</h3>
<div>
   
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.DataAge)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DataAge)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.HoraAge)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.HoraAge)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Cliente)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Cliente.PessoaId)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Corretor)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Corretor.PessoaId)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="AgendamentoId" />
        <input type="submit" value="Delete" class="btn price-button" />
        <a asp-action="Index"> <i class="bi bi-x-circle btn-outline-prima "></i> </a>
    </form>
</div>
</div>
