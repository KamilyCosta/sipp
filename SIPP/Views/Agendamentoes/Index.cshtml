@model IEnumerable<SIPP.Models.Agendamento>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DataAge)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HoraAge)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Cliente)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Corretor)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DataAge)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HoraAge)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Cliente.PessoaId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Corretor.PessoaId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.AgendamentoId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.AgendamentoId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.AgendamentoId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
